{
  "name": "msf-cli",
  "version": "1.2.0",
  "description": "My Sync Folders - sync files between folders based on watchman",
  "main": "index.js",
  "bin": {
    "msf": "lib/cli.js"
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run clean && npm run compile",
    "compile": "tsc",
    "start": "npm run compile -- --watch",
    "release": "npm test && npm run build && standard-version && git push --follow-tags origin master && npm publish",
    "test": "echo \"no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocwind/msf-cli.git"
  },
  "keywords": [
    "watchman",
    "sync"
  ],
  "author": "Roc",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "fb-watchman": "^2.0.1",
    "fs-extra": "^9.1.0",
    "ignore": "^5.1.8",
    "wait-ready": "^0.5.3",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/fb-watchman": "2.0.0",
    "@types/fs-extra": "9.0.11",
    "@types/node": "14.17.0",
    "@types/yargs": "16.0.3",
    "standard-version": "9.3.0",
    "typescript": "4.2.4"
  }
}
